{
  "name": "webdriverio_taf",
  "version": "1.0.0",
  "description": "WebdriverIO TAF for a Shopping cart app",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "start-server": "npm run start --prefix ../app",
    "pretest": "npm run lint-fix",
    "test": "wdio run ./wdio.conf.js",
    "start-and-test": "start-server-and-test \"npm run start-server\" 3000 \"npm test\"",
    "generate-report": "allure generate artifacts/allure-results --clean -o artifacts/allure-report",
    "open-report": "allure open artifacts/allure-report",
    "get-report": "run-s generate-report open-report"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zentestuken/atm_advanced.git"
  },
  "keywords": [
    "WebdriverIO",
    "Node.js",
    "test",
    "automation"
  ],
  "author": "Yauhen Viazau",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zentestuken/atm_advanced/issues"
  },
  "homepage": "https://github.com/zentestuken/atm_advanced#readme",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@eslint/json": "^0.13.2",
    "@wdio/allure-reporter": "^9.19.2",
    "@wdio/cli": "^9.19.2",
    "@wdio/local-runner": "^9.19.2",
    "@wdio/mocha-framework": "^9.19.2",
    "@wdio/spec-reporter": "^9.19.2",
    "allure-commandline": "^2.34.1",
    "eslint": "^9.35.0",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^2.1.0"
  }
}
